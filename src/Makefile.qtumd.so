include Makefile

##################################################################
test/test_bitcoin_local_rpc.o: test/test_bitcoin_local_rpc.cpp
	$(AM_V_CXX)$(CXX) $(DEFS) $(DEFAULT_INCLUDES) $(INCLUDES) \
	-I${JAVA_HOME}/include -I${JAVA_HOME}/include/darwin -I${JAVA_HOME}/include/linux \
	$(test_test_qtum_CPPFLAGS) $(CPPFLAGS) $(test_test_qtum_CXXFLAGS) $(CXXFLAGS) \
	-MT test/test_bitcoin_local_rpc.o -MD -MP -MF test/$(DEPDIR)/test_bitcoin_local_rpc.Tpo -c -o test/test_bitcoin_local_rpc.o test/test_bitcoin_local_rpc.cpp

qtumrpc: $(qtumd_OBJECTS) $(qtumd_DEPENDENCIES) $(EXTRA_qtumd_DEPENDENCIES) rpc/libbitcoin_cli_a-client.o test/test_bitcoin_local_rpc.o
	$(AM_V_CXXLD)$(qtumd_LINK) $(qtumd_OBJECTS) $(qtumd_LDADD) $(LIBS) rpc/libbitcoin_cli_a-client.o test/test_bitcoin_local_rpc.o

qtumd.dylib: $(qtumd_OBJECTS) $(qtumd_DEPENDENCIES) $(EXTRA_qtumd_DEPENDENCIES) rpc/libbitcoin_cli_a-client.o test/test_bitcoin_local_rpc.o
	$(AM_V_CXXLD)$(qtumd_LINK) $(qtumd_OBJECTS) $(qtumd_LDADD) $(LIBS) rpc/libbitcoin_cli_a-client.o test/test_bitcoin_local_rpc.o -dynamiclib

qtumd.so: $(qtumd_OBJECTS) $(qtumd_DEPENDENCIES) $(EXTRA_qtumd_DEPENDENCIES) rpc/libbitcoin_cli_a-client.o test/test_bitcoin_local_rpc.o
	$(AM_V_CXXLD)$(qtumd_LINK) $(qtumd_OBJECTS) $(qtumd_LDADD) $(LIBS) rpc/libbitcoin_cli_a-client.o test/test_bitcoin_local_rpc.o -shared

